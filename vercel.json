{
    "version": 2,
    "builds": [
        {
            "src": "web_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "web_project/wsgi.py"
        }
    ],
    "env": {
        "DEBUG": "False"
    }
}